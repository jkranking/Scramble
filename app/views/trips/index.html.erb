<ul>
  <% @trips.each do |trip| %>
    <li>
      <div class="trip-link">
        <%= link_to trip.try(:name), trip_path(trip) %></div>
      <div class="map-pic">

      <img src="https://maps.googleapis.com/maps/api/staticmap?center=<%= trip.latitude %>,<%= trip.longitude%>&zoom=<%= trip.zoom %>&size=600x300&maptype=terrain&path=geodesic:true|<%=static_path(trip)%><%=ENV["MAPS_KEY"]%>"></div>
    </li>
  <% end %>
</ul>
